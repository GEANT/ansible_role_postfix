# Postfix configuration file
# DO NOT EDIT
# {{ ansible_managed }}
{% for param, value in postfix_main  %}
{% if value is iterable and value is not string %}
{% if value.0.path is defined %}
{{ param }} = {{ value.0.path }}
{% else %}
{{ param }} = {{ value | sort | join (" ") }}
{% endif %}
{% else %}
{{ param }} = {{ value }}
{% endif %}
{% endfor %}
